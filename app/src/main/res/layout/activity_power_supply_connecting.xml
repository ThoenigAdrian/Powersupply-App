<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="advancedtech.nglabornetzgeraet.PowerSupplyConnecting">

    <Button
        android:id="@+id/decrease_voltage_channel_1"
        android:layout_width="45dp"
        android:layout_height="44dp"
        android:text="-"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/decrease_current_channel_1"
        android:layout_marginStart="32dp"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="11dp"
        app:layout_constraintLeft_toLeftOf="@+id/increase_voltage_channel_1"
        android:layout_marginLeft="37dp" />

    <Button
        android:id="@+id/increase_voltage_channel_1"
        android:layout_width="37dp"
        android:layout_height="42dp"
        android:text="+"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/increase_current_channel_1"
        android:layout_marginStart="48dp"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="13dp"
        app:layout_constraintLeft_toLeftOf="@+id/channel_1_heading"
        android:layout_marginLeft="43dp" />

    <Button
        android:id="@+id/connectionStatus"
        android:layout_width="137dp"
        android:layout_height="40dp"
        android:background="@drawable/colored_unclickable_button"
        android:text="@string/connecting_str"
        android:textColor="@android:color/white"
        android:textSize="12sp"
        android:textAllCaps="false"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="16dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintHorizontal_bias="0.502"
        tools:layout_editor_absoluteX="125dp" />

    <TextView
        android:id="@+id/channel_2_heading"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/channel_2"
        android:textColor="@android:color/black"
        android:textSize="20sp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="@+id/decrease_voltage_channel_1"
        android:layout_marginEnd="55dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="108dp"
        android:layout_marginBottom="109dp"
        app:layout_constraintTop_toTopOf="@+id/connectionStatus"
        android:layout_marginRight="55dp" />

    <TextView
        android:id="@+id/channel_1_heading"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/channel_1"
        android:textColor="@android:color/black"
        android:textSize="20sp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintBottom_creator="1"
        android:layout_marginStart="43dp"
        app:layout_constraintBottom_toBottomOf="@+id/channel_2_heading"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="@+id/channel_2_heading"
        android:layout_marginLeft="43dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:text="@string/example_current_channel_2"
        android:background="@drawable/textbox_with_borders"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="@+id/current_channel_1"
        app:layout_constraintRight_toRightOf="@+id/textView4"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/textView4"
        app:layout_constraintTop_toTopOf="@+id/decrease_current_channel_1" />

    <TextView
        android:id="@+id/current_channel_1"
        android:layout_width="51dp"
        android:layout_height="29dp"
        android:background="@drawable/textbox_with_borders"
        android:text="@string/example_current_channel_1"
        app:layout_constraintRight_toLeftOf="@+id/increase_current_channel_1"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        android:layout_marginStart="16dp"
        app:layout_constraintBottom_toBottomOf="@+id/increase_current_channel_1"
        android:layout_marginEnd="24dp"
        android:layout_marginTop="19dp"
        app:layout_constraintTop_toBottomOf="@+id/voltage_channel_1"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="19dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="24dp" />

    <TextView
        android:id="@+id/voltage_channel_1"
        android:layout_width="0dp"
        android:layout_height="28dp"
        android:background="@drawable/textbox_with_borders"
        android:text="@string/example_voltage_channel_1"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="@+id/increase_current_channel_1"
        app:layout_constraintRight_toRightOf="@+id/current_channel_1"
        android:layout_marginTop="66dp"
        app:layout_constraintTop_toBottomOf="@+id/channel_1_heading"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="67dp"
        app:layout_constraintLeft_toLeftOf="@+id/current_channel_1" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="50dp"
        android:layout_height="30dp"
        android:text="@string/example_voltage_channel_2"
        android:background="@drawable/textbox_with_borders"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        android:layout_marginStart="104dp"
        app:layout_constraintBottom_toBottomOf="@+id/decrease_current_channel_1"
        android:layout_marginEnd="109dp"
        app:layout_constraintRight_toRightOf="@+id/increase_voltage_channel_2"
        android:layout_marginTop="66dp"
        app:layout_constraintTop_toBottomOf="@+id/channel_2_heading"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="65dp"
        app:layout_constraintLeft_toLeftOf="@+id/decrease_voltage_channel_1"
        android:layout_marginLeft="104dp"
        android:layout_marginRight="109dp" />

    <Button
        android:id="@+id/decrease_current_channel_1"
        android:layout_width="45dp"
        android:layout_height="38dp"
        android:text="-"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="@+id/decrease_voltage_channel_1"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="202dp"
        app:layout_constraintLeft_toLeftOf="@+id/decrease_voltage_channel_1"
        app:layout_constraintHorizontal_bias="1.0" />

    <Button
        android:id="@+id/decrease_current_channel_2"
        android:layout_width="36dp"
        android:layout_height="43dp"
        android:text="-"
        app:layout_constraintLeft_toRightOf="@+id/increase_current_channel_2"
        android:layout_marginLeft="-128dp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintBottom_creator="1"
        android:layout_marginStart="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/decrease_current_channel_1"
        app:layout_constraintTop_toTopOf="@+id/textView3"
        app:layout_constraintVertical_bias="1.0" />

    <Button
        android:id="@+id/decrease_voltage_channel_2"
        android:layout_width="38dp"
        android:layout_height="46dp"
        android:text="-"
        android:layout_marginEnd="1dp"
        android:layout_marginRight="1dp"
        tools:layout_editor_absoluteY="199dp"
        tools:layout_editor_absoluteX="338dp" />

    <Button
        android:id="@+id/increase_voltage_channel_2"
        android:layout_width="40dp"
        android:layout_height="43dp"
        android:text="+"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/increase_current_channel_2"
        android:layout_marginStart="35dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/channel_2_heading"
        android:layout_marginLeft="27dp"
        android:layout_marginBottom="28dp" />

    <Button
        android:id="@+id/increase_current_channel_2"
        android:layout_width="43dp"
        android:layout_height="44dp"
        android:text="+"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="@+id/increase_voltage_channel_2"
        app:layout_constraintBaseline_toBaselineOf="@+id/decrease_current_channel_2"
        tools:layout_constraintBaseline_creator="1"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/increase_voltage_channel_2"
        app:layout_constraintHorizontal_bias="0.0" />

    <Button
        android:id="@+id/increase_current_channel_1"
        android:layout_width="36dp"
        android:layout_height="39dp"
        android:text="+"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="@+id/increase_voltage_channel_1"
        app:layout_constraintBaseline_toBaselineOf="@+id/decrease_current_channel_1"
        tools:layout_constraintBaseline_creator="1"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/increase_voltage_channel_1"
        app:layout_constraintHorizontal_bias="0.0" />


</android.support.constraint.ConstraintLayout>
